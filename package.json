        "@parcel/transformer-typescript-types": "2.7.0",
        "@types/jest": "^29.0.3",
        "@types/svg-parser": "^2.0.3",
        "@types/svg-path-parser": "^1.1.3",
        "benchmark": "^2.1.4",
        "buffer": "^6.0.3",
        "jest": "^29.0.3",
        "jest-bench": "^28.1.3",
        "parcel": "^2.7.0",
        "parcel-reporter-exec": "^0.0.7",
        "prettier": "2.7.1",
